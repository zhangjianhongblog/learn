# Editor configuration, see https://editorconfig.org/

# 是否是顶级配置文件，设置为true的时候才会停止搜索.editorconfig文件
root = true

[*]
# 编码格式 utf-8 latin utf-8-bom utf-16be utf-16le
charset = utf-8
# 缩进方式 tab space
indent_style = tab
# 缩进大小
indent_size = 4
# 是否让文件以空行结束
insert_final_newline = true
# 是否删除行尾空格
trim_trailing_whitespace = true
